{
  "expo": {
    "name": "الشني",
    "slug": "Shini",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ex-shni-logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.shareef.dweikat.alaqsa",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    }
  }
}
